@prefix : <http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix schema: <https://schema.org/> .
@base <http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology> .

<http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology> rdf:type owl:Ontology ;
                                                                          <http://purl.org/dc/elements/1.1/creator> "Andrei Condurache"@en ;
                                                                          rdfs:label "Architectural Patterns Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#accesses
:accesses rdf:type owl:ObjectProperty ;
          rdfs:domain :Component ;
          rdfs:range :StorageSystem ;
          rdfs:label "accesses" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#canBeDesignedUsing
:canBeDesignedUsing rdf:type owl:ObjectProperty ;
                    rdfs:domain :Component ;
                    rdfs:range :ArchitecturalPattern ;
                    rdfs:label "can Be Designed Using" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#couplingBetweenComponents
:couplingBetweenComponents rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain :ArchitecturalPattern ;
                           rdfs:range :Coupling ;
                           rdfs:label "coupling Between Components" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasAntiPattern
:hasAntiPattern rdf:type owl:ObjectProperty ;
                rdfs:domain :ArchitecturalPattern ;
                rdfs:range :Anti-Pattern ;
                rdfs:label "has Anti-Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasArchitectureCharacteristic
:hasArchitectureCharacteristic rdf:type owl:ObjectProperty ;
                               owl:inverseOf :isCharacteristicOf ;
                               rdfs:domain :ArchitecturalPattern ;
                               rdfs:range :ArchitectureCharacteristic ;
                               rdfs:comment "describes what type of Component an Architectural Pattern can have"@en ;
                               rdfs:label "has Architecture Characteristic" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isComponentOf ;
              rdfs:range :Component ;
              owl:propertyChainAxiom ( :hasTopology
                                       :hasComponent
                                     ) ;
              rdfs:label "has Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasGranularity
:hasGranularity rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf owl:topObjectProperty ;
                rdfs:domain :Component ;
                rdfs:range :ComponentGranularity ;
                rdfs:comment "specifies what Granularity a Component has."@en ;
                rdfs:label "has Granularity" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasPartitioning
:hasPartitioning rdf:type owl:ObjectProperty ;
                 rdfs:range :Partitioning ;
                 rdfs:comment "specifies that an Architectural Pattern has a particular Partitioning."@en ;
                 rdfs:label "has Partitioning" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasTopology
:hasTopology rdf:type owl:ObjectProperty ;
             rdfs:domain :ArchitecturalPattern ;
             rdfs:range :Topology ;
             rdfs:label "has Topology" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#includes
:includes rdf:type owl:ObjectProperty ;
          rdfs:domain :Component ;
          rdfs:range :Component ;
          rdfs:label "includes" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#inter-componentCommunication
:inter-componentCommunication rdf:type owl:ObjectProperty ;
                              rdfs:range :Communication ;
                              rdfs:comment "indicates the existing type of Communication between the components of an Architectural Pattern."@en ;
                              rdfs:label "inter-Component Communication" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#isCharacteristicOf
:isCharacteristicOf rdf:type owl:ObjectProperty ;
                    rdfs:label "is Characteristic Of" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#isComponentOf
:isComponentOf rdf:type owl:ObjectProperty ;
               rdfs:label "is Component Of" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#isSuitableFor
:isSuitableFor rdf:type owl:ObjectProperty ;
               rdfs:domain :ArchitecturalPattern ;
               rdfs:range :SoftwareApplication ;
               rdfs:label "is Suitable For" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#respectedBy
:respectedBy rdf:type owl:ObjectProperty ;
             owl:inverseOf :respectsPrinciple ;
             rdfs:label "respected By" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#respectsPrinciple
:respectsPrinciple rdf:type owl:ObjectProperty ;
                   rdfs:domain :ArchitecturalPattern ;
                   rdfs:range :ArchitecturalPrinciple ;
                   rdfs:comment "indicates the Architectural Principle that an architectural pattern respects."@en ;
                   rdfs:label "respects Principle" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usedBy
:usedBy rdf:type owl:ObjectProperty ;
        owl:inverseOf :usesTheDesignApproach ;
        rdfs:domain :DesignApproach ;
        rdfs:range :ArchitecturalPattern ;
        rdfs:label "used By" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usedInConjunctionWith
:usedInConjunctionWith rdf:type owl:ObjectProperty ,
                                owl:SymmetricProperty ;
                       rdfs:domain :ArchitecturalPattern ;
                       rdfs:range :ArchitecturalPattern ;
                       rdfs:comment "This property specifies an Architectural Pattern that can be used together with another Architectural Pattern"@en ;
                       rdfs:label "used In Conjunction With" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usesFrontendApproach
:usesFrontendApproach rdf:type owl:ObjectProperty ;
                      rdfs:domain :ArchitecturalPattern ;
                      rdfs:range :FrontendApproach ;
                      rdfs:label "uses Frontend Approach" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usesPattern
:usesPattern rdf:type owl:ObjectProperty ;
             rdfs:domain :ArchitecturalPattern ;
             rdfs:range :Pattern ;
             rdfs:comment "specifies the Pattern that Architectural Pattern uses."@en ;
             rdfs:label "uses Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usesStorageSystem
:usesStorageSystem rdf:type owl:ObjectProperty ;
                   rdfs:domain :ArchitecturalPattern ;
                   rdfs:range :StorageSystem ;
                   rdfs:label "uses Storage System" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#usesTheDesignApproach
:usesTheDesignApproach rdf:type owl:ObjectProperty ;
                       rdfs:domain :ArchitecturalPattern ;
                       rdfs:range :DesignApproach ;
                       rdfs:label "uses The Design Approach" .


#################################################################
#    Data properties
#################################################################

###  http://schema.org/alternateName
<http://schema.org/alternateName> rdf:type owl:DatatypeProperty ;
                                  rdfs:range xsd:string ;
                                  rdfs:comment "indicates what other name an Architectural Pattern is known by."@en ;
                                  rdfs:label "alternate Name" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#hasMultipleDeployableUnits
:hasMultipleDeployableUnits rdf:type owl:DatatypeProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain :ArchitecturalPattern ;
                            rdfs:range xsd:boolean ;
                            rdfs:comment "specifies true or false if an Architectural Pattern is composed of multiple deployable units."@en ;
                            rdfs:label "has Multiple Deployable Units" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ratingValue
:ratingValue rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;
             rdfs:domain :ArchitectureCharacteristic ;
             rdfs:range xsd:integer ;
             rdfs:label "rating Value" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#requiresOrchestration
:requiresOrchestration rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:range xsd:boolean ;
                       rdfs:comment "specifies if orchestration is necessary for an Architectural Pattern."@en ;
                       rdfs:label "requires Orchestration" .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#APILayer
:APILayer rdf:type owl:Class ;
          rdfs:subClassOf :Component ;
          rdfs:label "API Layer" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Anti-Pattern
:Anti-Pattern rdf:type owl:Class ;
              rdfs:comment "Refers to common design or implementation pitfalls that should be avoided."@en ;
              rdfs:label "Anti-Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ArchitecturalPattern
:ArchitecturalPattern rdf:type owl:Class ;
                      rdfs:subClassOf [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasArchitectureCharacteristic ;
                                        owl:someValuesFrom :ArchitectureCharacteristic
                                      ] ,
                                      [ rdf:type owl:Restriction ;
                                        owl:onProperty :hasComponent ;
                                        owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                        owl:onClass :Component
                                      ] ;
                      rdfs:comment "The class of Architectural patterns."@en ;
                      rdfs:label "Architectural Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ArchitecturalPrinciple
:ArchitecturalPrinciple rdf:type owl:Class ;
                        rdfs:comment "describes the class of guidelines for making architectural decisions that lead to clean and maintainable applications."@en ;
                        rdfs:label "Architectural Principle" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ArchitectureCharacteristic
:ArchitectureCharacteristic rdf:type owl:Class ;
                            rdfs:comment "refers to an important aspect of the system which isn’t related to the domain functionality."@en ;
                            rdfs:label "Architecture Characteristic" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#AsynchronousCommunication
:AsynchronousCommunication rdf:type owl:Class ;
                           rdfs:subClassOf :Communication ;
                           owl:disjointWith :SynchronousCommunication ;
                           rdfs:label "Asynchronous Communication" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#AsynchronousMessaging
:AsynchronousMessaging rdf:type owl:Class ;
                       rdfs:subClassOf :AsynchronousCommunication ;
                       rdfs:label "Asynchronous Messaging" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#BusinessApplication
:BusinessApplication rdf:type owl:Class ;
                     rdfs:subClassOf :SoftwareApplication ;
                     rdfs:label "Business Application" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Communication
:Communication rdf:type owl:Class ;
               rdfs:comment "describes how components communicate with one another."@en ;
               rdfs:label "Communication" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Component
:Component rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasGranularity ;
                             owl:someValuesFrom :ComponentGranularity
                           ] ;
           rdfs:comment "refers to a fundamental building block in architecture that performs specific business or infrastructure tasks."@en ;
           rdfs:label "Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ComponentGranularity
:ComponentGranularity rdf:type owl:Class ;
                      rdfs:comment "refers to the deployment unit’s size."@en ;
                      rdfs:label "Component Granularity" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#CoreSystem
:CoreSystem rdf:type owl:Class ;
            rdfs:subClassOf :Component .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Coupling
:Coupling rdf:type owl:Class ;
          rdfs:comment "represents the extent to which components know about each other."@en ;
          rdfs:label "Coupling" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Deployability
:Deployability rdf:type owl:Class ;
               rdfs:subClassOf :ArchitectureCharacteristic ;
               rdfs:label "Deployability" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#DesignApproach
:DesignApproach rdf:type owl:Class ;
                rdfs:comment "illustrates a methodology or set of principles intended to provide assistance in the design process."@en ;
                rdfs:label "Design Approach" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#DiscoveryComponent
:DiscoveryComponent rdf:type owl:Class ;
                    rdfs:subClassOf :Component ;
                    rdfs:label "Discovery Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#DistributedArchitecturalPattern
:DistributedArchitecturalPattern rdf:type owl:Class ;
                                 rdfs:subClassOf :ArchitecturalPattern ,
                                                 [ rdf:type owl:Restriction ;
                                                   owl:onProperty :hasMultipleDeployableUnits ;
                                                   owl:hasValue "true"^^xsd:boolean
                                                 ] ;
                                 owl:disjointWith :MonolithicArchitecturalPattern ;
                                 rdfs:label "Distributed Architectural Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Domain-CentricComponent
:Domain-CentricComponent rdf:type owl:Class ;
                         rdfs:subClassOf :Component ;
                         rdfs:label "Domain-Centric Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Elasticity
:Elasticity rdf:type owl:Class ;
            rdfs:subClassOf :ArchitectureCharacteristic ;
            rdfs:label "Elasticity" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf :Component ;
       rdfs:label "Event" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Event_Processing_Component
:Event_Processing_Component rdf:type owl:Class ;
                            rdfs:subClassOf :Component ;
                            rdfs:label "Event Processing Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Fault_Tolerance
:Fault_Tolerance rdf:type owl:Class ;
                 rdfs:subClassOf :ArchitectureCharacteristic ;
                 rdfs:label "Fault Tolerance" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Filter_Component
:Filter_Component rdf:type owl:Class ;
                  rdfs:subClassOf :Component ;
                  rdfs:label "Filter Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#FrontendApproach
:FrontendApproach rdf:type owl:Class ;
                  rdfs:comment "defines how the user interface can be designed within the system"@en ;
                  rdfs:label "Frontend Approach" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#LayerComponent
:LayerComponent rdf:type owl:Class ;
                rdfs:subClassOf :Component ;
                rdfs:label "Layer Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#LayeredCoreSystem
:LayeredCoreSystem rdf:type owl:Class ;
                   rdfs:subClassOf :CoreSystem ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPartitioning ;
                                     owl:hasValue :TechnicalPartitioning
                                   ] .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#MobileApplication
:MobileApplication rdf:type owl:Class ;
                   rdfs:subClassOf :SoftwareApplication ;
                   rdfs:label "Mobile Application" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ModularCoreSystem
:ModularCoreSystem rdf:type owl:Class ;
                   rdfs:subClassOf :CoreSystem ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :hasPartitioning ;
                                     owl:hasValue :DomainPartitioning
                                   ] .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Modularity
:Modularity rdf:type owl:Class ;
            rdfs:subClassOf :ArchitectureCharacteristic ;
            rdfs:label "Modularity" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Module
:Module rdf:type owl:Class ;
        rdfs:subClassOf :Component ;
        rdfs:label "Module" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#MonolithicArchitecturalPattern
:MonolithicArchitecturalPattern rdf:type owl:Class ;
                                rdfs:subClassOf :ArchitecturalPattern ,
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty :hasMultipleDeployableUnits ;
                                                  owl:hasValue "false"^^xsd:boolean
                                                ] ;
                                rdfs:label "Monolithic Architectural Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Orchestration
:Orchestration rdf:type owl:Class ;
               rdfs:label "Orchestration" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Overall_Cost
:Overall_Cost rdf:type owl:Class ;
              rdfs:subClassOf :ArchitectureCharacteristic ;
              rdfs:label "Overall Cost" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Partitioning
:Partitioning rdf:type owl:Class ;
              rdfs:comment "defines the organization of the system components according to technical usage."@en ;
              rdfs:label "Partioning" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Pattern
:Pattern rdf:type owl:Class ;
         rdfs:comment "defines a reusable solution to a problem that occurs in a particular context."@en ;
         rdfs:label "Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Performance
:Performance rdf:type owl:Class ;
             rdfs:subClassOf :ArchitectureCharacteristic ;
             rdfs:label "Performance" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Point-to-pointCommunication
:Point-to-pointCommunication rdf:type owl:Class ;
                             rdfs:subClassOf :SynchronousCommunication ;
                             rdfs:label "Point-to-point Communication" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Reliability
:Reliability rdf:type owl:Class ;
             rdfs:subClassOf :ArchitectureCharacteristic ;
             rdfs:label "Reliability" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#RemoteProcedureInvocation
:RemoteProcedureInvocation rdf:type owl:Class ;
                           rdfs:subClassOf :SynchronousCommunication ;
                           rdfs:label "Remote Procedure Invocation" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Scalability
:Scalability rdf:type owl:Class ;
             rdfs:subClassOf :ArchitectureCharacteristic ;
             rdfs:label "Scalability" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ServiceComponent
:ServiceComponent rdf:type owl:Class ;
                  rdfs:subClassOf :Component ;
                  rdfs:label "Service Component" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Simplicity
:Simplicity rdf:type owl:Class ;
            rdfs:subClassOf :ArchitectureCharacteristic .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#SoftwareApplication
:SoftwareApplication rdf:type owl:Class ;
                     rdfs:comment "the class of software applications."@en ;
                     rdfs:label "Software Application" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#StorageSystem
:StorageSystem rdf:type owl:Class ;
               rdfs:label "Storage System" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#SynchronousCommunication
:SynchronousCommunication rdf:type owl:Class ;
                          rdfs:subClassOf :Communication ;
                          rdfs:label "Synchronous Communication" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Testability
:Testability rdf:type owl:Class ;
             rdfs:subClassOf :ArchitectureCharacteristic ;
             rdfs:label "Testability" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Topology
:Topology rdf:type owl:Class ;
          rdfs:comment "illustrates various topology variants an architecture may have."@en ;
          rdfs:label "Topology" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#WebApplication
:WebApplication rdf:type owl:Class ;
                rdfs:subClassOf :SoftwareApplication ;
                rdfs:label "Web Application" .


#################################################################
#    Individuals
#################################################################

###  http://dbpedia.org/resource/Event-driven_architecture
dbr:Event-driven_architecture rdf:type owl:NamedIndividual ;
                              owl:sameAs :Event-Driven_Architecture_Pattern .


###  http://dbpedia.org/resource/Microservices
dbr:Microservices rdf:type owl:NamedIndividual ;
                  owl:sameAs :Microservices_Architecture_Pattern .


###  http://dbpedia.org/resource/Service-oriented_architecture
dbr:Service-oriented_architecture rdf:type owl:NamedIndividual ;
                                  owl:sameAs :Service_Oriented_Architecture_Pattern .


###  http://dbpedia.org/resource/Space-based_architecture
dbr:Space-based_architecture rdf:type owl:NamedIndividual ;
                             owl:sameAs :Space-Based_Architecture_Pattern .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#APIGateway
:APIGateway rdf:type owl:NamedIndividual ,
                     :APILayer ;
            rdfs:label "API Gateway" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#APIGatewayPattern
:APIGatewayPattern rdf:type owl:NamedIndividual ,
                            :Pattern ;
                   rdfs:label "API Gateway Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#API_Facade_Layer
:API_Facade_Layer rdf:type owl:NamedIndividual ,
                           :LayerComponent ;
                  rdfs:label "API Facade Layer" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#API_REST-based_topology
:API_REST-based_topology rdf:type owl:NamedIndividual ,
                                  :Topology ;
                         rdfs:label "API REST-based Topology" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ApplicationService
:ApplicationService rdf:type owl:NamedIndividual ,
                             :ServiceComponent ;
                    rdfs:label "Application Service" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Application_REST-based_topology
:Application_REST-based_topology rdf:type owl:NamedIndividual ,
                                          :Topology ;
                                 rdfs:label "Application REST-based Topology" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ArchitectureByImplicationAnti-Pattern
:ArchitectureByImplicationAnti-Pattern rdf:type owl:NamedIndividual ,
                                                :Anti-Pattern ;
                                       rdfs:label "Architecture By Implication Anti-Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#ArchitectureSinkholeAnti-Pattern
:ArchitectureSinkholeAnti-Pattern rdf:type owl:NamedIndividual ,
                                           :Anti-Pattern ;
                                  rdfs:label "Architecture Sinkhole Anti-Pattern" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#BackendsForFrontends
:BackendsForFrontends rdf:type owl:NamedIndividual ,
                               :Pattern ;
                      rdfs:label "Backends for Frontends" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Bounded_contexts
:Bounded_contexts rdf:type owl:NamedIndividual ,
                           :ArchitecturalPrinciple ;
                  rdfs:label "Bounded contexts" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Broker_Topology
:Broker_Topology rdf:type owl:NamedIndividual ,
                          :Topology ;
                 :hasComponent :Event_Broker ,
                               :Event_Processor ,
                               :InitiatingEvent ,
                               :Processing_Event ;
                 :inter-componentCommunication :MessageQueues ;
                 :requiresOrchestration "false"^^xsd:boolean ;
                 rdfs:label "Broker Topology" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#BusinessDelegate
:BusinessDelegate rdf:type owl:NamedIndividual ,
                           :Pattern ;
                  rdfs:label "Business Delegate" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#BusinessService
:BusinessService rdf:type owl:NamedIndividual ,
                          :ServiceComponent ;
                 rdfs:label "Business Service" .


###  http://www.semanticweb.org/ontologies/webArchitecturalPatternsOntology#Business_Layer
